{
    "name": "idez/laravel-bankly-sdk",
    "description": "This is my package laravel-bankly-sdk",
    "keywords": [
        "Idez",
        "laravel",
        "laravel-bankly-sdk"
    ],
    "homepage": "https://github.com/idezdigital/laravel-bankly-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Idez Digital",
            "email": "developers@idez.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/contracts": "^8.0|^9.0",
        "illuminate/http": "^8.0|^9.0",
        "illuminate/support": "^8.0|^9.0",
        "psr/http-message": "^1.0",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^1.0|^2.0.1",
        "orchestra/testbench": "^6.0|^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Idez\\Bankly\\": "src",
            "Idez\\Bankly\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Idez\\Bankly\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Idez\\Bankly\\BanklyServiceProvider"
            ],
            "aliases": {
                "Bankly": "Idez\\Bankly\\Facades\\Bankly"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
